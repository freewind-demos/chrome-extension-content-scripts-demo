{
  "manifest_version": 2,

  "name": "Insert Text by content_scripts",
  "description": "Insert Text to end of the page by content_scripts",
  "version": "1.0",

  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["hello.js"]
    }
  ],
  "permissions": [
    "tabs", "http://*/*","https://*/*"
  ]
}